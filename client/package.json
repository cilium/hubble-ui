  "scripts": {
    "copy-types-schema": "npm run generate-types && npm run copy-shared",
    "generate-types": "rm -f ./src/graphqlTypes.ts && gql-gen",
    "copy-shared": "rm -rf src/shared ; cp -R ../server/src/shared src/shared",
    "build-css": "node-sass-chokidar  src/blueprint.scss -o src/ --importer=node_modules/node-sass-package-importer/dist/cli.js --functions=src/sass-inline-svg.js",
    "start-js": "node scripts/start.js",
    "start": "npm run copy-types-schema && npm run build-css && npm run start-js",
    "build": "CI=false npm run copy-types-schema && npm run build-css && node scripts/build.js && mv build/index.html .",
    "test": "npm run copy-types-schema && node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ]
}
