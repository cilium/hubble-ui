{
  "private": true,
  "name": "hubble-ui-client",
  "scripts": {
    "test": "jest",
    "cleanup": "rm -rf ./build",
    "upgrade-deps": "../node_modules/.bin/npm-check --update --save-exact",
    "watch": "npm run build-blueprint && NODE_ENV=development webpack --progress",
    "build": "npm run build-blueprint && NODE_ENV=production webpack",
    "build-blueprint": "node-sass-chokidar src/blueprint.scss -o src --importer=./node_modules/node-sass-package-importer/dist/cli.js --functions=scripts/sass-inline-svg.js"
  },
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "@blueprintjs/core": "3.26.0",
    "@blueprintjs/datetime": "3.16.1",
    "@blueprintjs/icons": "3.15.1",
    "@blueprintjs/select": "3.12.2",
    "@reach/router": "1.3.3",
    "classnames": "2.2.6",
    "core-js": "3.6.5",
    "d3": "5.16.0",
    "date-fns": "2.12.0",
    "google-protobuf": "3.11.4",
    "grpc-web": "1.0.7",
    "lodash": "4.17.15",
    "mobx": "5.15.4",
    "mobx-react": "6.2.2",
    "object-hash": "2.0.3",
    "prop-types": "15.7.2",
    "query-string": "6.12.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-spring": "8.0.27",
    "react-use-gesture": "7.0.15"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@testing-library/react": "10.0.4",
    "@types/classnames": "2.2.10",
    "@types/d3": "5.7.2",
    "@types/google-protobuf": "3.7.2",
    "@types/jest": "25.2.1",
    "@types/lodash": "4.14.150",
    "@types/object-hash": "1.3.1",
    "@types/prop-types": "15.7.3",
    "@types/reach__router": "1.3.5",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-test-renderer": "16.9.2",
    "autoprefixer": "9.7.6",
    "babel-jest": "25.4.0",
    "babel-loader": "8.1.0",
    "circular-dependency-plugin": "5.2.0",
    "css-loader": "3.5.3",
    "dotenv-webpack": "1.7.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.5.4",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.0",
    "node-sass-chokidar": "1.4.0",
    "node-sass-package-importer": "5.3.2",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "3.0.0",
    "react-dev-utils": "10.2.1",
    "react-test-renderer": "16.13.1",
    "sass-inline-svg": "1.2.3",
    "sass-loader": "8.0.2",
    "source-map-explorer": "2.4.2",
    "source-map-loader": "0.2.4",
    "svg-react-loader": "0.4.6",
    "ts-jest": "25.4.0",
    "ts-loader": "7.0.1",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.8.3",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)",
      "<rootDir>/../common/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/../common/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1",
      "^~common/(.*)$": "<rootDir>/../common/src/$1",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    }
  }
}
