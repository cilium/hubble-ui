{
  "name": "hubble-ui",
  "private": true,
  "scripts": {
    "test": "jest",
    "postinstall": "node ./scripts/install-grpc-deps",
    "watch:server": "servor ./server/public index.html 8080",
    "watch:client": "./scripts/build-blueprint.sh && NODE_ENV=development webpack --progress",
    "watch": "run-p watch:*",
    "build": "./scripts/generate-proto.sh && ./scripts/build-blueprint.sh && NODE_ENV=production webpack",
    "lint:ts": "eslint --ignore-path .gitignore \"./src/**/*.{ts,tsx,js,jsx,mjs}\"",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:scss": "stylelint --ignore-path .gitignore \"./**/*.scss\" --syntax scss",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "upgrade-deps": "npx npm-check --update --save-exact"
  },
  "lint-staged": {
    "./src/!(proto)/**/*.{ts,tsx,js,jsx,mjs}": [
      "npm run --silent lint:ts:fix"
    ],
    "./src/**/*.scss": [
      "npm run --silent lint:scss:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
