    "google-protobuf": "3.21.2",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "mobx": "6.12.1",
    "mobx-react": "9.1.0",
    "pluralize": "8.0.0",
    "prop-types": "15.8.1",
    "query-string": "9.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.3",
    "react-window": "1.8.10",
    "url-parse": "1.5.10"
  },
