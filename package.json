    "lodash": "4.17.21",
    "mobx": "6.13.6",
    "mobx-react": "9.2.0",
    "pluralize": "8.0.0",
    "prop-types": "15.8.1",
    "query-string": "9.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "7.1.5",
    "react-window": "1.8.11",
    "url-parse": "1.5.10"
